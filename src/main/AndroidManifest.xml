<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dash.a1511n">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".application.DashApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.WebViewActivity" />
        <activity android:name=".view.activity.DetailActivity" />
        <activity android:name=".view.activity.ImageScaleActivity" />
        <activity android:name=".view.activity.ProductListActivity" />
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.UserSettingActivity" />
        <activity android:name=".view.activity.MyOrderActivity"></activity>
    </application>

</manifest>